{
  "name": "jp-proofreading-github",
  "version": "1.0.0",
  "description": "Japanese Document Proofreading System (GitHub Version)",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "curl http://localhost:3000",
    "git:init": "git init && git add . && git commit -m 'Initial commit'",
    "git:commit": "git add . && git commit -m",
    "git:status": "git status",
    "git:log": "git log --oneline"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "jszip": "^3.10.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "japanese",
    "proofreading",
    "document",
    "correction",
    "ayumi"
  ],
  "author": "",
  "license": "MIT"
}